case $CURRENT_SHELL in
bash)
    test -r /usr/share/doc/fzf/examples/key-bindings.bash && . "$_"
    test -r /usr/share/doc/fzf/examples/completion.bash && . "$_"
    ;;
zsh)
    test -r /usr/share/doc/fzf/examples/key-bindings.zsh && . "$_"
    test -r /usr/share/doc/fzf/examples/completion.zsh && . "$_"
    ;;
esac
