		{
			"description": "[Amalgamation] Lock workstation",
			"manipulators": [
				{ "type": "basic", "from": { "key_code": "l", "modifiers": { "mandatory": ["option"] } }, "to": [{ "shell_command": "osascript -e 'tell application \"System Events\" to key code 12 using {control down, command down}'" }], "conditions": [{ "type": "frontmost_application_unless", "bundle_identifiers": ["^com\\.vmware\\.fusion$"] }] }
			]
		},
